{
  "name": "estoque-hsi",
  "version": "1.0.0",
  "private": true,
  "description": "Sistema de Estoque de TI HSI - Monorepo",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo run clean && rimraf node_modules",
    "db:migrate": "npm run db:migrate --workspace=@estoque-hsi/db",
    "db:seed": "npm run db:seed --workspace=@estoque-hsi/db",
    "db:studio": "npm run db:studio --workspace=@estoque-hsi/db",
    "import:inventario": "tsx scripts/import-hsi-inventario.ts",
    "test:import:api": "tsx scripts/test-import-hsi-api.ts"
  },
  "devDependencies": {
    "@turbo/gen": "^2.1.3",
    "chardet": "^2.1.1",
    "csv-parse": "^6.1.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "npm@10.9.0",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
